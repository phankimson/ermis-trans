<table>
    <tr>
        <td class="row-label-responsive"><label>{{ antl.get('inventory_voucher.inventory_receipt') }}</label></td>
        <td>
        <select class="droplist large" name="inventory_receipt">
                <option readonly selected value="0">--Select--</option>
                  {% for m in stock %}
                    <option value="{{ m.id }}">{{ shortif( lang == 'vn' , m.code + ' - ' + m.name ,m.code + ' - ' +  m.name_en )}}</option>
                   {% endfor %}
            </select>
        </td>
        <td></td>
        <td class="row-label-responsive"></td>
        <td><label>{{ antl.get('inventory_voucher.date_voucher') }}</label></td>
        <td colspan="2"><input type="text" data-type="date" class="k-textbox date-picker" name="date_voucher" /></td>
    </tr>
