        <td colspan="2"><input type="text" data-type="date" class="k-textbox date-picker" name="date_account" /></td>
    </tr>-->
    <tr>
    <td><label>{{ antl.get('transport.title') }}</label></td>
    <td colspan="2">
    <select class="droplist large" name="transport">
            <option readonly selected value="0">--Select--</option>
              {% for m in transport %}
                <option value="{{ m.id }}">{{ shortif( lang == 'vn' , m.code + ' - ' + m.name ,m.code + ' - ' +  m.name_en )}}</option>
               {% endfor %}
        </select>
    </td>
        <td class="row-label-responsive"></td>
        <td><label>{{ antl.get('inventory_voucher.voucher') }}</label></td>
        <td colspan="2"><input type="text" class="k-textbox medium voucher" name="voucher" /></td>
    </tr>
    <tr>
        <td><label>{{ antl.get('driver.title') }}</label></td>
        <td>
        <select class="droplist large" name="driver">
                <option readonly selected value="0">--Select--</option>
                  {% for m in driver %}
                    <option value="{{ m.id }}">{{ shortif( lang == 'vn' , m.code + ' - ' + m.name ,m.code + ' - ' +  m.name_en )}}</option>
                   {% endfor %}
            </select>
        </td>
        <td></td>
        <td class="row-label-responsive"></td>
        <td><label>{{ antl.get('transport.rental_fees') }}</label></td>
        <td colspan="2">
            <input type="text" class="medium number-price" name="rental_fees" />
        </td>
    </tr>

    <tr>
      <td><label>{{ antl.get('inventory_voucher.receiver') }}</label></td>
      <td><input type="text" class="k-textbox xxlarge" name="traders" /></td>
      <td></td>
      <td class="row-label-responsive"></td>
      <td><label>{{ antl.get('transport.hire_surcharge') }}</label></td>
      <td colspan="2">
          <input type="text" class="medium number-price" name="hire_surcharge" />
      </td>
    </tr>
    <tr>
        <td><label>{{ antl.get('inventory_voucher.description') }}</label></td>
        <td><input type="text" class="k-textbox xxlarge" name="description" /></td>
        <td></td>
        <td class="row-label-responsive"></td>
        <td><label>{{ antl.get('transport.type_surcharge') }}</label></td>
        <td colspan="2">
            <select class="droplist medium" name="surcharge">
                <option readonly selected value="0">--Select--</option>
                  {% for m in surcharge %}
                    <option value="{{ m.id }}">{{ shortif( lang == 'vn' , m.code + ' - ' + m.name ,m.code + ' - ' +  m.name_en )}}</option>
                   {% endfor %}
            </select>
        </td>
    </tr>

    <tr>
        <td><label>{{ antl.get('inventory_voucher.barcode_item') }}</label></td>
        <td colspan="2">
            <span class="k-textbox k-space-right xxlarge">
                <input type="text" id="barcode" name="barcode" />
                <a href="javascript:;" style="right : 10px"  class="k-icon k-i-search barcode">&nbsp;</a>
            </span>
        </td>
    </tr>
