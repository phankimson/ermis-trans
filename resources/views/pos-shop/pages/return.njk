                                    <tr>
                                        <td class="row-label"><label>{{ antl.get('inventory_voucher.description') }}</label></td>
                                        <td ><input type="text" value="" name="description" class="k-textbox xlarge-responsive not_load" /></td>
                                    </tr>
                                    <tr>
                                        <td class="row-label"><label>{{ antl.get('return.receiver_fullname') }}</label></td>
                                        <td ><input type="text" value="" name="receiver_fullname" class="k-textbox xlarge-responsive" /></td>
                                    </tr>
                                    <tr>
