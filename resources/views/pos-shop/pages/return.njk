                                <table id="form-action-2">
                                <tr>
                                    <td class="row-label"><label>{{ antl.get('inventory_voucher.voucher') }}</label></td>
                                    <td><input type="text" class="voucher k-textbox xlarge-responsive disabled not_load" readonly name="voucher"/></td>
                                </tr>
                                <tr>
                                    <td class="row-label"><label>{{ antl.get('inventory_voucher.date_voucher') }}</label></td>
                                    <td ><input type="text" value="" name="date_voucher" readonly class="k-textbox xlarge-responsive disabled date not_load" /></td>
