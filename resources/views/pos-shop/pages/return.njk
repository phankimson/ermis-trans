                                  <li ><a href="javascript:;">{{ antl.get('return.info') }}</a></li>
                                  <li ><a href="javascript:;">{{ antl.get('return.sender1') }}</a></li>
                                  <li ><a href="javascript:;">{{ antl.get('return.receiver') }}</a></li>
                                </ul>
                            </div>
                        </div>
