                                        <td ><input type="text" value="" name="receiver_phone" class="k-textbox xlarge-responsive" /></td>
                                    </tr>
                                    <tr>
                                        <td class="row-label"><label>{{ antl.get('return.identity_card') }}</label></td>
                                        <td ><input type="text" value="" name="identity_card" class="k-textbox xlarge-responsive" /></td>
                                    </tr>
                                </table>
                                </li>
                                <li aria-hidden="true">
                                <!-- Tabs 2 -->
                                <table id="form-load" >
                                <tr>
                                    <td class="row-label"><label>{{ antl.get('store.voucher') }}</label></td>
                                    <td><input type="text" class="voucher k-textbox xlarge-responsive disabled" readonly name="code"/></td>
                                </tr>
                                <tr>
                                    <td class="row-label"><label>{{ antl.get('store.date_voucher') }}</label></td>
                                    <td ><input type="text" value="" name="date_voucher" readonly class="k-textbox xlarge-responsive disabled date" /></td>
                                </tr>
                                <tr>
                                    <td class="row-label"><label>{{ antl.get('store.transport_station_send') }}</label></td>
                                    <td ><input type="text" value="" name="transport_station_send" readonly class="k-textbox xlarge-responsive disabled" /></td>
                                </tr>
                                <tr>
                                    <td class="row-label"><label>{{ antl.get('store.transport_station_receive') }}</label></td>
                                    <td ><input type="text" value="" name="transport_station_receive" readonly class="k-textbox xlarge-responsive disabled" /></td>
                                </tr>
                                    <tr>
                                        <td class="row-label"><label>{{ antl.get('store.parcel_volumes') }}</label></td>
                                        <td ><input type="text" value="" name="parcel_volumes" readonly class="k-textbox xlarge-responsive disabled" /></td>
                                    </tr>
                                    <tr>
                                        <td class="row-label"><label>{{ antl.get('store.size') }}</label></td>
                                        <td ><input type="text" value="" name="size" readonly class="k-textbox xlarge-responsive disabled" /></td>
                                    </tr>
                                    <tr>
                                        <td class="row-label"><label>{{ antl.get('store.name') }}</label></td>
                                        <td ><input type="text" value="" name="name" readonly class="k-textbox xlarge-responsive disabled" /></td>
                                    </tr>
                                    <tr>
                                        <td class="row-label"><label>{{ antl.get('store.lot_number') }}</label></td>
                                        <td ><input type="text" value="" name="lot_number" readonly class="k-textbox xlarge-responsive disabled" /></td>
                                    </tr>
                                    <tr>
                                        <td class="row-label"><label>{{ antl.get('store.note') }}</label></td>
                                        <td ><input type="text" value="" name="note" readonly class="k-textbox xlarge-responsive disabled" /></td>
                                    </tr>
                                </table>
                                </li>
                                <li>
                                <table>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.fullname') }} * </label></td>
                                      <td ><input type="text" value="" name="sender_fullname" readonly class="k-textbox large disabled" /></td>
                                  </tr>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.phone') }} * </label></td>
                                      <td ><input type="text" value="" name="sender_phone" readonly class="k-textbox large disabled" /></td>
                                  </tr>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.email') }} * </label></td>
                                      <td ><input type="text" value="" name="sender_email" readonly class="k-textbox large disabled" /></td>
                                  </tr>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.address') }} * </label></td>
                                      <td ><input type="text" value="" name="sender_address" readonly class="k-textbox large disabled" /></td>
                                  </tr>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.city') }} </label></td>
                                      <td >
                                      <input type="text" value="" name="sender_city" readonly class="k-textbox large disabled" />
                                      </td>
                                  </tr>
                              </table>
                                </li>
                                <li>
                                <table>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.fullname') }} * </label></td>
                                      <td ><input type="text" value="" name="receiver_fullname" readonly class="k-textbox large disabled" /></td>
                                  </tr>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.phone') }} * </label></td>
                                      <td ><input type="text" value="" name="receiver_phone" readonly class="k-textbox large disabled" /></td>
                                  </tr>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.email') }} * </label></td>
                                      <td ><input type="text" value="" name="receiver_email" readonly class="k-textbox large disabled" /></td>
                                  </tr>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.address') }} * </label></td>
                                      <td ><input type="text" value="" name="receiver_address" readonly class="k-textbox large disabled" /></td>
                                  </tr>
                                  <tr>
                                      <td class="row-label-medium"><label>{{ antl.get('store.city') }} </label></td>
                                      <td >
                                      <input type="text" value="" name="receiver_city" readonly class="k-textbox large disabled" />
                                      </td>
                                  </tr>
                              </table>
                                </li>
                            </ul>
                </div>
            </div>
        </div>

    </div>
</div>



<!-- Barcode FORM -->
<div id="form-window-barcode" style="display:none">
<select class="droplist not_null" name="filter_field">
    <option readonly selected value="">--Select--</option>
    <option value="code">{{ antl.get('store.voucher') }}</option>
    <option value="name">{{ antl.get('store.name') }}</option>
    <option value="date_voucher">{{ antl.get('store.date_voucher') }}</option>
    <option value="sender_fullname">{{ antl.get('return.sender_fullname') }}</option>
    <option value="sender_phone">{{ antl.get('return.sender_phone') }}</option>
    <option value="receiver_fullname">{{ antl.get('return.receiver_fullname') }}</option>
    <option value="receiver_phone">{{ antl.get('return.receiver_phone') }}</option>
</select>
<input type="text" name="filter_value" class="k-textbox">
<a href="javascript:;" class="uk-margin-left-30 k-button k-primary" id="search_data" data-uk-tooltip=""><i class="md-18 material-icons md-color-white">search</i>{{ antl.get('action.search') }}</a>

<div class="uk-margin-medium-top"></div>

<div id="grid_barcode"></div>

<div class="uk-margin" style="float : right">
    <a href="javascript:;" class="k-button k-primary choose_barcode" data-uk-tooltip title="{{ antl.get('action.choose') }}"><i class="md-18 material-icons md-color-white">done</i>{{ antl.get('action.choose') }}</a>
    <a href="javascript:;" class="k-button k-primary cancel-window" data-uk-tooltip title="{{ antl.get('action.close') }}"><i class="md-18 material-icons md-color-white">not_interested</i>{{ antl.get('action.close') }}</a>
</div>
</div>
<!-- Barcode FORM -->
<div id="print"></div>

{% endblock %}
{% block scripts %}
<script>
jQuery(document).ready(function () {
    Ermis.per = JSON.parse('{{ permission | safe }}');
    Ermis.link = "{{ key }}";
    Ermis.room = "{{ room }}";
    Ermis.voucher  =  {{ voucher | json | safe }};
    Ermis.columns    = [{"field" :"id", hidden : true},
                         { "field" : "code","title" : "{{ antl.get('store.voucher') }}" ,"width" : "130px"  },
                         { "field" : "name","title" : "{{ antl.get('store.name') }}","width" : "150px"  },
                         { "field" : "date_voucher","title" : "{{ antl.get('store.date_voucher') }}","width" : "150px",template : "#=FormatDate(date_voucher)#" },
                         { "field" : "transport_station_send","title" : "{{ antl.get('store.transport_station_send') }}","width" : "100px" },
                         { "field" : "transport_station_receive","title" : "{{ antl.get('store.transport_station_receive') }}","width" : "100px" },
                         { "field" : "sender_fullname","title" : "{{ antl.get('return.sender_fullname') }}" ,"width" : "200px"},
                         { "field" : "sender_phone","title" : "{{ antl.get('return.sender_phone') }}" ,"width" : "150px"},
                         { "field" : "receiver_fullname","title" : "{{ antl.get('return.receiver_fullname') }}","width" : "200px" },
                         { "field" : "receiver_phone","title" : "{{ antl.get('return.receiver_phone') }}" ,"width" : "150px" }];


     Ermis.field = {
         id : {field :"id" ,defaultValue: 0},
         code : { editable: false},
         name : {editable: false },
         date_voucher :      {editable: false },
         transport_station_send :      {editable: false },
         transport_station_receive : {editable: false },
         sender_fullname : {editable: false },
         sender_phone:     {editable: false },
         receiver_fullname:    {editable: false },
         receiver_phone:     {editable: false}
     };

     Ermis.columns_barcode    = [{title: 'Select All',headerTemplate: '<input type="checkbox" id="header-chb-b" class="k-checkbox"><label class="k-checkbox-label" for="header-chb-b"></label>',template: function(dataItem){return '<input type="checkbox" id="c-'+dataItem.id+'" class="k-checkbox"><label class="k-checkbox-label" for="c-'+dataItem.id+'"></label>'},width: 80},
                          { "field" : "code","title" : "{{ antl.get('store.voucher') }}" ,"width" : "130px"  },
                          { "field" : "name","title" : "{{ antl.get('store.name') }}","width" : "150px"  },
                          { "field" : "date_voucher","title" : "{{ antl.get('store.date_voucher') }}","width" : "150px",template : "#=FormatDate(date_voucher)#" },
                          { "field" : "sender_fullname","title" : "{{ antl.get('return.sender_fullname') }}" ,"width" : "200px"},
                          { "field" : "sender_phone","title" : "{{ antl.get('return.sender_phone') }}" ,"width" : "150px"},
                          { "field" : "receiver_fullname","title" : "{{ antl.get('return.receiver_fullname') }}","width" : "200px" },
                          { "field" : "receiver_phone","title" : "{{ antl.get('return.receiver_phone') }}" ,"width" : "150px" }];

});
</script>
<script src="{{ url('scripts/uikit.min.js') }}"></script>
<script src="{{ url('scripts/ukit_pagination.js') }}"></script>
<script src="{{ url('library/jquery.print/jQuery.print.js') }}"></script>
<script src="{{ url('scripts/addon/frontend/store_return.js') }}"></script>
{% endblock %}
